<div class="tool-box">
<div class="menu fixed-action-btn horizontal" >
    <a class="btn-floating btn-large blue-grey lighten-4">
      <i class="large material-icons">menu</i>
    </a>
    <ul>
      <% var cIcon = "airplay" %>
      <% theme.menu.forEach(function(item,i){ %> 
      <% if(item.icon) {cIcon = item.icon} %>
      <li>
        <a class="btn-floating tooltipped <%= item.color %>" data-position="top" data-delay="50" data-tooltip="<%= __('menu.' +item.name) %>" href="<%= item.url%>">
          <i class="material-icons"><%= cIcon %></i>
        </a>
      </li>
      
      <%}) %>
    </ul>
</div>
<div class="back-top">
  <a class="btn-floating btn-large" href="#!"><i class="material-icons">up</i> </a>
</div>
</div>
